

<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Rose Bud Thorn</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
  </head>
 
  <body>
    <div class="container">
      <nav class="navbar navbar-default navbar-inverse navbar-fixed-top" role="navigation">
        <div class="navbar-header">
          <a class="navbar-brand" href="/">RBT</a>
          <a class="navbar-brand" href="/profile">Me</a>
        </div>
      </nav>
      <br /><br /><br />
      <h1 class="text-center">Rose Bud Thorn</h1>
      <br />

      {{#each rbt}}
        <center>
          <img src={{photo_user}} style="width: 150px; height: 150px;">
          <h3>{{name}}</h3>
          <div class="hidden" id="hash">{{photo_rbt}}</div>

        </center>
        <center><a href="/edit" class="btn btn-default">Update Status</a></center>
        
        <table border="0" class="table" style="margin-top:30px;">
          <tbody>
            <tr style="height: 150px">
              <td id="spin" style="width: 150px">
                  <img class="pano" id="pano" style="width: 150px" />
              </td>
              <td>
                <span class="label label-danger">Rose</span>
                {{rose}}
              </td>
            </tr>
            <tr style="height: 150px">
              <td style="text-align: center;">
                <span class="label label-success">Bud</span>
                {{bud}}
              </td>
              <td>
                <b></b>
                <span class="label label-warning">Thorn</span>
                {{thorn}}
              </td>
            </tr>
          </tbody>
        </table>
        <br />
      {{/each}}
    </div>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdn.firebase.com/v0/firebase.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/spin.js/1.2.7/spin.min.js"></script>
    <script type="text/javascript" src="/js/sha256.js"></script>
    <script type="text/javascript" src="/js/fbRetrive.js"></script>
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="/js/bootstrap.min.js"></script>

    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="/js/bootstrap.min.js"></script>
</body>
</html>